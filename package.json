{
    "name": "vscode-npm-scripts",
    "displayName": "NPM-Scripts",
    "description": "View and run NPM scripts in the sidebar.",
    "version": "0.1.3",
    "publisher": "traBpUkciP",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other"
    ],
    "license": "MIT",
    "activationEvents": [
        "onView:npmScripts"
    ],
    "main": "./out/src/extension",
    "icon": "media/file_type_npm.png",
    "repository": {
        "url": "https://github.com/Duroktar/vscode-npm-scripts"
    },
    "contributes": {
        "views": {
            "explorer": [
                {
                    "id": "npmScripts",
                    "name": "NPM Scripts"
                }
            ]
        },
        "commands": [
            {
                "command": "npmScripts.executeCommand",
                "title": "Run Script"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/node": "*",
        "typescript": "^2.1.4",
        "vscode": "^1.0.0"
    }
}